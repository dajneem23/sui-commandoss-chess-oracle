{
    "name": "demo",
    "private": true,
    "version": "1.0.0",
    "description": "Nest TypeScript boilerplate repository",
    "author": "dajneem23 <dajneem23@gmail.com>",
    "license": "MIT",
    "type": "commonjs",
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=10.0.0"
    },
    "scripts": {
        "format": "prettier --write \"apps/**/*.ts\" ",
        "schema:drop": "ts-node node_modules/typeorm/cli.js schema:drop -f ormconfig",
        "checkTs": "tsc --noEmit",
        "typeorm": "ts-node --transpileOnly -r tsconfig-paths/register ./node_modules/typeorm/cli",
        "migration:run": "npm run typeorm migration:run -- -d ormconfig.ts",
        "migration:show": "npm run typeorm migration:show -- -d ormconfig.ts",
        "migration:seed": "npm run typeorm migration:run --  -d ./src/config/typeorm.seed.ts",
        "migration:show-seed": "npm run typeorm migration:show --  -d ./src/config/typeorm.seed.ts",
        "migration:generate": "npm run typeorm -- -d ormconfig.ts migration:generate ./migrations/apps",
        "migration:create": "npm run typeorm -- migration:create /migrations/",
        "migration:revert": "npm run typeorm -- -d .ormconfig.ts migration:revert",
        "start:dev": "cross-env NODE_ENV=development  NODE_OPTIONS=--max-old-space-size=18192 nest start --watch ",
        "start:prod": "cross-env NODE_ENV=production  NODE_OPTIONS=--max-old-space-size=8192 node dist/src/main.js",
        "build": "nest build",
        "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
        "lint:fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./e2e/jest-e2e.json"
    },
    "dependencies": {
        "@apollo/federation": "^0.38.1",
        "@apollo/gateway": "^2.5.1",
        "@apollo/subgraph": "^2.5.1",
        "@nestjs/apollo": "^12.0.7",
        "@nestjs/axios": "^3.0.0",
        "@nestjs/bullmq": "^11.0.2",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/cqrs": "^10.0.0",
        "@nestjs/graphql": "^12.0.8",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/microservices": "^10.0.0",
        "@nestjs/mongoose": "^10.1.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/platform-socket.io": "^10.4.5",
        "@nestjs/schedule": "^4.1.1",
        "@nestjs/swagger": "^7.1.2",
        "@nestjs/terminus": "^10.0.0",
        "@nestjs/typeorm": "^10.0.0",
        "@nestjs/websockets": "^10.4.6",
        "@socket.io/redis-adapter": "^8.3.0",
        "aws-sdk": "^2.1419.0",
        "axios": "^1.6.0",
        "class-transformer": "0.5.1",
        "class-validator": "^0.14.0",
        "cross-env": "^7.0.3",
        "cross-spawn": "^7.0.6",
        "dotenv": "16.4.5",
        "express": "^4.21.1",
        "express-rate-limit": "^7.4.1",
        "graphql": "^16.8.1",
        "helmet": "^8.0.0",
        "i": "^0.3.7",
        "joi": "^17.13.3",
        "JSONStream": "^1.3.5",
        "keccak": "^3.0.4",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "mime-types": "~2.1.24",
        "mongoose": "^8.8.0",
        "morgan": "^1.10",
        "mysql2": "^3.5.2",
        "nest-winston": "^1.9.7",
        "nestjs-joi": "^1.10.1",
        "npm": "^11.4.1",
        "pg": "^8.16.0",
        "pgn-parser": "^2.2.1",
        "redis": "^4.7.0",
        "rimraf": "^6.0.1",
        "rxjs": "^7.8.1",
        "socket.io": "^4.8.0",
        "swagger-ui-express": "^5.0.0",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.1",
        "typeorm": "^0.3.20",
        "uuid": "10.0.0",
        "winston": "^3.2.1",
        "winston-daily-rotate-file": "^3.10.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.7",
        "@commitlint/config-conventional": "19.5.0",
        "@commitlint/lint": "^17.6.7",
        "@nestjs/cli": "^11.0.7",
        "@nestjs/testing": "^10.0.0",
        "@types/eslint": "^9.6.1",
        "@types/eslint__js": "^8.42.3",
        "@types/express": "^4.17.17",
        "@types/express-rate-limit": "^6.0.0",
        "@types/jest": "^29.0.0",
        "@types/lodash": "^4.14.134",
        "@types/mime-types": "^2.1.0",
        "@types/morgan": "^1.7.35",
        "@types/node": "^16.14.0",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.11.0",
        "@typescript-eslint/parser": "^8.11.0",
        "@typescript-eslint/utils": "^8.12.2",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-n": "^17.12.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.11.0",
        "husky": "9.1.6",
        "jest": "^29.0.0",
        "lint-staged": "15.2.10",
        "prettier": "^3.0.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-morph": "24.0.0",
        "tsc-watch": "^6.0.4",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.12.2",
        "zx": "^8.5.4"
    },
    "lint-staged": {
        "*.@(ts)": [
            "eslint --fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run build && lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "keywords": [
        "nestjs",
        "boilerpale",
        "microservice",
        "ddd",
        "cqrs"
    ]
}
